{
    "all": true,
    "check-coverage": false,    
    "include": [
      "**/*.js",
      "**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "**/rollup.config.js",
      "build/"
    ],
    "extension": [
      ".js",
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "./coverage",
    "branches": 15,
    "lines": 40,
    "functions": 35,
    "statements": 40    
}
