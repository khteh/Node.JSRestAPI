{
  "name": "node.jsrestapi",
  "version": "1.0.0",
  "private": true,
  "exports": "./bin/www.js",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js --ignore-path .gitignore ./",
    "dev": "./node_modules/.bin/nodemon --trace-deprecation --ignore node_modules/ bin/www.js",
    "start": "export NODE_ENV=dev&& node bin/www.js",
    "test": "export NODE_ENV=test&& ./node_modules/.bin/mocha tests --timeout 10000 --recursive --check-leaks --exit --reporter mocha-junit-reporter --reporter-options mochaFile=test_reports/mocha/test-results.xml",
    "cover": "./node_modules/.bin/nyc --reporter=lcov --reporter=text --reporter=text-summary --reporter=html npm test",
    "sonarqube": "./node_modules/.bin/mocha --reporter mocha-sonarqube-reporter tests --reporter-options output=test_reports/sonarqube/test_results.xml"
  },
  "dependencies": {
    "ansi-regex": "^6.0.1",
    "async": "^3.2.4",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "debug": "~4.3.1",
    "ejs": "~3.1.6",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "http-errors": "~2.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "spdy": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.19.0",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-sonarqube-reporter": "^1.0.2",
    "nodemon": "^2.0.18",
    "nyc": "^15.1.0",
    "sonarqube-scanner": "^2.8.1"
  }
}
