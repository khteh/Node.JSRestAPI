{
  "name": "webapi",
  "version": "1.0.0",
  "description": "",
  "author": "Teh Kok How",
  "license": "ISC",
  "type": "module",
  "exports": "./build/index.js",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.json && npm run rollup",
    "build:watch": "nodemon -e ts --exec \"npm run build\" --ignore build/** --ignore tests/**",
    "rollup": "rollup -c",
    "dev": "export NODE_ENV=development&& concurrently \"npx tsc --watch\" \"nodemon -q build/server.js\"",
    "proto_old": "npx proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=grpc/client grpc/proto/*.proto && protoc -I=./grpc/proto ./grpc/proto/*.proto --js_out=import_style=commonjs:./grpc/client --grpc-web_out=import_style=typescript,mode=grpcweb:./grpc/client",
    "proto": "npx grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./grpc/client --grpc_out=grpc_js:./grpc/client --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./grpc/proto ./grpc/proto/*.proto && npx protoc --plugin=protoc-gen-ts=`which protoc-gen-ts` --ts_out=grpc_js:./grpc/client -I ./grpc/proto ./grpc/proto/*.proto",
    "start": "node build/server.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "infrastructure": "^1.4.4",
    "inversify": "^6.0.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rotating-file-stream": "^3.2.3",
    "spdy": "^4.0.2",
    "webapi.core": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/compression": "latest",
    "@types/cookie-parser": "latest",
    "@types/cors": "latest",
    "@types/debug": "latest",
    "@types/google-protobuf": "latest",
    "@types/http-errors": "latest",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "@types/spdy": "latest",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "nodemon": "^3.1.4",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-ts": "^3.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "keywords": [],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=19.8.1"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/build"
        ]
      }
    }
  }
}
